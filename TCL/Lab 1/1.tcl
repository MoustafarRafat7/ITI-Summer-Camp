set SrcA 20
set SrcB 4
set Result [expr $SrcA + $SrcB ]
puts "Sum = $Result"
set Result [expr $SrcA - $SrcB ]
puts "Subtraction = $Result"
set Result [expr $SrcA * $SrcB ]
puts "multiplication = $Result"
set Result [expr $SrcA / $SrcB ]
puts "Division = $Result"
set Result [expr $SrcA ** $SrcB ]
puts "Exponential = $Result"
set Result [expr $SrcA % $SrcB ]
puts "remainder = $Result"
set Result [expr $SrcA << $SrcB ]
puts "shift left = $Result"
set Result [expr $SrcA >> $SrcB ]
puts "shift right = $Result"

